{
  "expo": {
    "name": "Quotic",
    "slug": "quotic",
    "privacy": "public",
    "sdkVersion": "34.0.0",
    "platforms": ["android"],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "githubUrl": "githubUrl",
    "primaryColor": "#dccd62",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#dccd62"
    },
    "updates": {
      "enabled": true,
      "fallbackToCacheTimeout": 30000
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.madebydayo.quotic"
    }
  }
}
